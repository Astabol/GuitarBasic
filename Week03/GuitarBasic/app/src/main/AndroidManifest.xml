<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.guitarbasic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".GscaleChords"
            android:label="G Scale Chords List"></activity>
        <activity android:name=".FscaleChords" android:label="F Scale Chords List" />
        <activity android:name=".EscaleChords" android:label="E Scale Chords List"/>
        <activity android:name=".BscaleChords" android:label="B Scale Chords List"/>
        <activity android:name=".AscaleChords" android:label="A Scale Chords List"/>
        <activity
            android:name=".DscaleChords"
            android:label="D Scale Chords List"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".CscaleChords"
            android:label="C Scale Chord List"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".FlashScreen"
            android:theme="@style/AppTheme.NoActionBarFlash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>